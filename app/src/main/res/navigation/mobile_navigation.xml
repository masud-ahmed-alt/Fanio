<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment android:id="@+id/navigation_home"
        android:name="www.fanio.com.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home"/>

    <fragment android:id="@+id/navigation_facebook"
        android:name="www.fanio.com.ui.facebook.FacebookFragment"
        android:label="Facebook"
        tools:layout="@layout/fragment_facebook"/>

    <fragment android:id="@+id/navigation_youtube"
        android:name="www.fanio.com.ui.youtube.YoutubeFragment"
        android:label="Youtube"
        tools:layout="@layout/fragment_youtube"/>

    <fragment android:id="@+id/navigation_instagram"
        android:name="www.fanio.com.ui.instagram.InstagramFragment"
        android:label="Instagram"
        tools:layout="@layout/fragment_instagram"/>

    <fragment android:id="@+id/navigation_about"
        android:name="www.fanio.com.ui.about.AboutFragment"
        android:label="About Us"
        tools:layout="@layout/fragment_about"/>
</navigation>